<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="437.0" prefWidth="327.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.stockmanage102Udeeka.controller.LoginFormController">
   <children>
      <AnchorPane fx:id="rootLogin" layoutY="5.0" prefHeight="437.0" prefWidth="327.0">
         <children>
            <Label layoutX="126.0" layoutY="24.0" text="LOGIN" wrapText="true">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <TextField fx:id="txtUser" layoutX="20.0" layoutY="91.0" prefHeight="56.0" prefWidth="290.0" promptText="Enter your user name here" />
            <TextField fx:id="txtPw" layoutX="21.0" layoutY="163.0" prefHeight="56.0" prefWidth="290.0" promptText="Password" />
            <Button layoutX="21.0" layoutY="237.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="40.0" prefWidth="290.0" style="-fx-background-color: Yellow;" text="Login">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Label layoutX="21.0" layoutY="296.0" text="Don't have a account?">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label layoutX="175.0" layoutY="296.0" text="Register here" textFill="#1966c9">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
